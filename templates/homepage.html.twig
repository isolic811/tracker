{% extends 'base.html.twig' %}

{% block title %}Homepage!{% endblock %}

{% block body %}
{% endblock %}
    {% block javascripts %}

        <script>
            $(document).ready(function() {
                var start = new Date();

                $(window).unload(function() {
                    var end = new Date();
                    $.ajax({
                        url: "log.php",
                        data: {'timeSpent': end - start},
                        async: false
                    })
                });
            });
        </script>

    {% endblock %}
